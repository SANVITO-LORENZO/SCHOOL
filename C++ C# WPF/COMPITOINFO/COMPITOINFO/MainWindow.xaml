        <Label Content="NOME SQUADRA OSPITE" HorizontalAlignment="Left" Margin="277,72,0,0" VerticalAlignment="Top" Width="168" FontSize="14"/>
        <Label Content="PROGRAMMA STATISTICHE RUGBY" HorizontalAlignment="Left" Margin="41,12,0,0" VerticalAlignment="Top" Width="416" FontWeight="Bold" FontSize="24"/>
        <Label Content="DATA DELLA PARTITA:" HorizontalAlignment="Left" Margin="41,189,0,0" VerticalAlignment="Top" Width="194" FontSize="14"/>
        <DatePicker x:Name="dataTimePicker" HorizontalAlignment="Left" Margin="265,191,0,0" VerticalAlignment="Top" Width="192"/>
        <TextBox x:Name="txt_ospite" HorizontalAlignment="Left" Margin="265,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="194" Height="51"/>

    </Grid>
</Window>
